{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended", ":dependencyDashboard"],
  "baseBranches": ["main"],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "branchConcurrentLimit": 0,
  "mode": "full",
  "flux": {
    "fileMatch": ["^.*\\flux.yaml$"]
  },
  "packageRules": [
    {
      "matchPackageNames": ["*"],
      "commitMessagePrefix": "feat(deps)"
    }
  ]
}
